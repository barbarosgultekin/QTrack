@using EntityLayer.Concrete
@model List<QRHistory>

@{
    Layout = null;
}


<link href="~/Layout/css/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/owl.carousel.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/owl.theme.default.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/tooplate-style.css" rel="stylesheet" type="text/css" />
<link rel="shortcut icon" href="~/Layout/images/favicon.png" type="image/x-icon">
<link href="~/Layout/css/components.min.css" rel="stylesheet" id="style_components" type="text/css" />
<link href="~/Layout/css/plugins.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/custom.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/datatables.min.css" rel="stylesheet" type="text/css" />
<link href="~/Layout/css/datatables.bootstrap.css" rel="stylesheet" type="text/css" />

<div style="margin-top:15px">
    <div class="col-md-12">
        <div class="portlet-body form">
            <div class="portlet light bordered">
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="icon-settings font-dark"></i>
                        <span class="caption-subject bold uppercase">QR Code History - @ViewBag.QR</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <table class="table table-striped table-bordered table-hover dt-responsive" width="100%" id="sample_3" cellspacing="0">
                        <thead>
                            <tr>
                                <th> QR Code</th>
                                <th class="none">ID</th>
                                <th class="none">Serial</th>
                                <th class="none">Company</th>
                                <th class="none">Product</th>
                                <th>Operation</th>
                                <th>Operation Date</th>
                                <th>Fill Date</th>
                                <th>ReFill Date</th>
                                <th class="none">Location</th>
                                <th class="none">Factory Date</th>
                                <th>Exp Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.QR.QRCODE</td>
                                    <td>@item.REFID</td>
                                    <td>@item.QR.Product.SERIAL</td>
                                    <td>@item.QR.Company.CNAME</td>
                                    <td>@item.QR.Product.PNAME</td>
                                    <td>@item.OPERATION</td>
                                    <td>@Convert.ToDateTime(item.OPERATIONDATE).ToString("dd/MM/yyyy")</td>
                                    <td>@Convert.ToDateTime(item.QR.FILLDATE).ToString("dd/MM/yyyy")</td>
                                    <td>@Convert.ToDateTime(item.QR.REFILLDATE).ToString("dd/MM/yyyy")</td>
                                    <td><a href="@item.QR.Product.LOCATION" target="_blank">Click By Location</a></td>
                                    <td>@Convert.ToDateTime(item.QR.FACTORYDATE).ToString("dd/MM/yyyy")</td>
                                    <td>@Convert.ToDateTime(item.QR.EXPDATE).ToString("dd/MM/yyyy")</td>

                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Layout/js/jquery.js"></script>
<script src="~/Layout/js/bootstrap.min.js"></script>
<script src="~/Layout/js/datatable.js" type="text/javascript"></script>
<script src="~/Layout/js/datatables.min.js" type="text/javascript"></script>
<script src="~/Layout/js/datatables.bootstrap.js" type="text/javascript"></script>
<script src="~/Layout/js/bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
<script src="~/Layout/js/jquery.slimscroll.min.js" type="text/javascript"></script>
<script src="~/Layout/js/table-datatables-responsive.min.js" type="text/javascript"></script>
<script src="~/Layout/js/app.min.js" type="text/javascript"></script>
